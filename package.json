{
  "name": "ens-app-v2",
  "version": "0.1.122",
  "workspaces": [
    "./functions"
  ],
  "scripts": {
    "dev": "NODE_NO_WARNINGS=1 next dev",
    "dev:nlocal": "NEXT_PUBLIC_PROVIDER=http://localhost:8545 NEXT_PUBLIC_AVUP_ENDPOINT=http://localhost:8787 pnpm dev",
    "dev:localname": "NEXT_PUBLIC_PROVIDER=\"http://$(\"hostname\"):8545\" && NEXT_PUBLIC_AVUP_ENDPOINT=\"http://$(\"hostname\"):8787\" && NEXT_PUBLIC_GRAPH_URI=\"http://$(\"hostname\"):8000/subgraphs/name/graphprotocol/ens\" && pnpm dev",
    "dev:glocal": "NEXT_PUBLIC_GRAPH_URI=http://localhost:8000/subgraphs/name/graphprotocol/ens pnpm dev:nlocal",
    "dev:gonline": "NEXT_PUBLIC_GRAPH_URI=https://api.thegraph.com/subgraphs/name/tateb/enssubdomaincount pnpm dev",
    "build": "next build",
    "build:local": "NODE_ENV=development next build",
    "build:glocal": "NEXT_PUBLIC_GRAPH_URI=http://localhost:8000/subgraphs/name/graphprotocol/ens NEXT_PUBLIC_PROVIDER=http://localhost:8545 next build",
    "build:localname": "NEXT_PUBLIC_PROVIDER=\"http://$(\"hostname\"):8545\" && NEXT_PUBLIC_GRAPH_URI=\"http://$(\"hostname\"):8000/subgraphs/name/graphprotocol/ens\" && pnpm build",
    "start": "next start",
    "buildandstart": "pnpm build && pnpm start",
    "buildandstart:glocal": "pnpm build:glocal && pnpm start",
    "tenv": "ens-test-env -a",
    "denv": "pnpm tenv start -ns -nb",
    "lint": "next lint && stylelint ./src/**/*.tsx",
    "lint:fix": "next lint --fix && stylelint --fix ./src/**/*.tsx",
    "export": "next export",
    "analyze": "ANALYZE=true pnpm build",
    "analyse": "pnpm analyze",
    "test": "jest --env=./custom-test-env.js",
    "test:watch": "jest --env=./custom-test-env.js --watch",
    "test:coverage": "jest --env=./custom-test-env.js --coverage",
    "synpress:start": "synpress run -cf ./synpress.json",
    "synpress:local": "synpress run -ne -cf ./synpress.json",
    "synpress:stateful": "SECRET_WORDS=\"test test test test test test test test test test test junk\" NETWORK_NAME=mainnet synpress run -cf ./synpress-stateful.json",
    "synpress:ci": "pnpm synpress:start --record --key aab18385-26dc-4b7c-8631-e91bc79f2ab2",
    "synpress:ci:stateful": "pnpm synpress:stateful --record --key aab18385-26dc-4b7c-8631-e91bc79f2ab2",
    "e2e": "E2E=true pnpm tenv start",
    "e2e:ci": "E2E=true CI=true pnpm tenv start",
    "e2e:stateful": "E2E=true pnpm buildandstart & wait-on http://localhost:3000 && pnpm synpress:stateful",
    "e2e:ci:stateful": "E2E=true pnpm buildandstart & wait-on http://localhost:3000 && pnpm synpress:ci:stateful",
    "postinstall": "husky install",
    "update:local:ensjs": "yalc add @ensdomains/ensjs && pnpm",
    "update:local:thorin": "yalc add @ensdomains/thorin && pnpm",
    "wrangle": "next build && next export && npx wrangler pages dev ./out",
    "wrangle:dev": "wrangler pages dev ./next",
    "generate:sitemaps": "node ./generate-site-map.mjs",
    "enode": "ganache --wallet.mnemonic=\"test test test test test test test test test test test junk\" --chain.chainId 1337 --chain.networkId 1337 --chain.time 1659500634000",
    "compose": "cp ./node_modules/@ensdomains/ens-test-env/src/docker-compose.yml ./docker-compose.yml"
  },
